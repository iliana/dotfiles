COLOR tty
TERM putty-256color
TERM rxvt-256color
TERM rxvt-unicode-256color
TERM rxvt-unicode256
TERM screen-256color
TERM xterm-256color
TERM gnome-256color
TERM st-256color
TERM tmux-256color

#NORMAL 00	# global default, no color code at all
#FILE 00	# normal file, use no color at all
RESET 0 # reset to "normal" color
DIR 38;5;117;1	# directory
LINK 38;5;117;3	# symbolic link (If you set this to 'target' instead of a
		# numerical value, the color is as for the file pointed to.)
MULTIHARDLINK 0 # regular file with more than one link
FIFO 38;5;228	# pipe
SOCK 38;5;212;3	# socket
DOOR 38;5;212;3	# door
BLK 38;5;228;1	# block device driver
CHR 38;5;228;1	# character device driver
ORPHAN 38;5;203;1  # symlink to nonexistent file, or non-stat'able file
MISSING 0 # ... and the files they point to
SETUID 48;5;196;38;5;15;1	# file that is setuid (u+s)
SETGID 48;5;11;38;5;16;1	# file that is setgid (g+s)
CAPABILITY 48;5;196;38;5;226;1	# file with capability
STICKY_OTHER_WRITABLE 48;5;84;38;5;236;1 # dir that is sticky and other-writable (+t,o+w)
OTHER_WRITABLE 48;5;84;38;5;236;1 # dir that is other-writable (o+w) and not sticky
STICKY 48;5;117;38;5;236;1	# dir with the sticky bit set (+t) and not other-writable

